<section class="container">
  <h2 class="text-center">Login Page</h2>

  <div class="row">
    <%= form_for @changeset, @action, [class: "col-md-6 offset-md-3"], fn f -> %>
      <%= input f, :username %>
      <%= input f, :password %>

      <%= submit "Submit", class: "btn btn-primary" %>
    <% end %>
  </div>
</section>