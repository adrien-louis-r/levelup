<h1>Show Credential</h1>

<ul>

  <li>
    <strong>Username:</strong>
    <%= @credential.username %>
  </li>

  <li>
    <strong>Password:</strong>
    <%= @credential.password %>
  </li>

  <li>
    <strong>Tenant:</strong>
    <%= @credential.tenant.title %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.credential_path(@conn, :edit, @credential) %></span>
<span><%= link "Back", to: Routes.credential_path(@conn, :index) %></span>
